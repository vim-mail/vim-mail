!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
. screenshort	README.md	/^## screenshort$/;"	h
. usage	README.md	/^## usage$/;"	h
.vim-mail	README.md	/^# vim-mail$/;"	h
b:current_syntax	syntax/mail.vim	/^let b:current_syntax = "mail"$/;"	v
build_header	autoload/mail.vim	/^function! s:build_header(...) abort$/;"	f
g:mail_command	plugin/mail.vim	/^let g:mail_command = 'telnet'$/;"	v
g:mail_directory	plugin/mail.vim	/^    let g:mail_directory = expand('~\/.vim-mail\/')$/;"	v
g:mail_method	plugin/mail.vim	/^let g:mail_method = 'imap'$/;"	v
g:mail_sending_address	plugin/mail.vim	/^let g:mail_sending_address = 'wsdjeg@163.com'$/;"	v
g:mail_sending_name	plugin/mail.vim	/^let g:mail_sending_name = 'Shidong Wang'$/;"	v
mail#client#connect	autoload/mail/client.vim	/^function! mail#client#connect(ip, port)$/;"	f
mail#client#send	autoload/mail/client.vim	/^function! mail#client#send(command)$/;"	f
mail#command#login	autoload/mail/command.vim	/^function! mail#command#login(username, password)$/;"	f
mail#list	autoload/mail.vim	/^function! mail#list() abort$/;"	f
mail#new	autoload/mail.vim	/^function! mail#new()$/;"	f
on_exit	autoload/mail/client.vim	/^function! s:on_exit(id, data, event) abort$/;"	f
on_stderr	autoload/mail/client.vim	/^function! s:on_stderr(id, data, event) abort$/;"	f
on_stdout	autoload/mail/client.vim	/^function! s:on_stdout(id, data, event) abort$/;"	f
s:JOB	autoload/mail/client.vim	/^let s:JOB = SpaceVim#api#import('job')$/;"	v
s:PASSWORD	autoload/mail/command.vim	/^let s:PASSWORD = SpaceVim#api#import('password')$/;"	v
s:headers	autoload/mail.vim	/^let s:headers = {$/;"	v
s:job_id	autoload/mail/client.vim	/^let s:job_id = 0$/;"	v
